{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./resources/dist",
        "rootDir": "./resources/ts",
        "moduleResolution": "bundler"
    },
    "include": [
        "resources/ts/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
