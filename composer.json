{
    "name": "modus-digital/modus-ui",
    "version": "0.0.1",
    "description": "A Component library based on TailwindCSS and Flowbite crafted by Modus",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.7",
        "orchestra/testbench": "^9.9"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ModusDigital\\ModusUI\\": "src/"
        }
    },
    "authors": [
        {
            "name": "AlexVanSteenhoven",
            "email": "alex@modus-digital.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ModusDigital\\ModusUI\\ModusUIServiceProvider"
            ]
        }
    }
}
